{
	"name": "a22-multy-app",
	"version": "0.1.0",
	"main": "dist/main.js",
	"scripts": {
		"clean": "rm -rf node_modules yarn.lock",
		"clean:lite": "rm -rf node_modules/a22-shared yarn.lock",
		"libs:up": "yarn upgrade a22-shared --latest",
		"build": "yarn libs:up && yarn && yarn tsc && yarn tsc -p tsconfig.preload.json",
		"build:prod": "yarn && yarn libs:up && yarn && yarn tsc",
		"go": "(electron . --no-sandbox) 2>&1 | grep -v 'eglQueryDeviceAttribEXT'",
		"go2": "yarn build && yarn tsc && electron .",
		"go3": "(yarn build && yarn tsc && electron .) 2>&1 | grep -v 'eglQueryDeviceAttribEXT'",
		"release:mac": "yarn build:prod && electron-builder --mac"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"type": "module",
	"devDependencies": {
		"@types/node": "^22.14.1",
		"electron": "~36.2.0",
		"electron-builder": "^26.0.12",
		"filesize": "^10.1.6",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"typescript": "^5.8.3",
		"zod": "3.24.3"
	},
	"build": {
		"appId": "com.yourapp.id",
		"productName": "a22-multy-app",
		"directories": {
			"output": "release-builds"
		},
		"files": [
			"dist/**/*",
			"node_modules/**/*",
			"package.json"
		],
		"asar": true,
		"win": {
			"target": "nsis"
		},
		"mac": {
			"target": "dmg",
			"category": "public.app-category.utilities"
		},
		"linux": {
			"target": [
				"AppImage",
				"deb"
			]
		},
		"extraResources": [
			{
				"from": "node_modules/electron-dl",
				"to": "node_modules/electron-dl"
			},
			{
				"from": "node_modules/uuid",
				"to": "node_modules/uuid"
			},
			{
				"from": "node_modules/extract-zip",
				"to": "node_modules/extract-zip"
			},
			{
				"from": "node_modules/lodash",
				"to": "node_modules/lodash"
			},
			{
				"from": "node_modules/filesize",
				"to": "node_modules/filesize"
			},
			{
				"from": "node_modules/mitt",
				"to": "node_modules/mitt"
			},
			{
				"from": "node_modules/zod",
				"to": "node_modules/zod"
			},
			{
				"from": "node_modules/a22-shared",
				"to": "a22-shared"
			}
		]
	},
	"dependencies": {
		"a22-shared": "file:../shared",
		"check-disk-space": "^3.4.0",
		"electron-dl": "^4.0.0",
		"electron-store": "^10.0.1",
		"extract-zip": "^2.0.1",
		"filesize": "^10.1.6",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"uuid": "^11.1.0",
		"zod": "3.24.3"
	},
	"description": ""
}
